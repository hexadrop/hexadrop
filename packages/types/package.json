{
	"name": "@hexadrop/types",
	"version": "1.0.3",
	"description": "Hexagonal architecture utils library",
	"keywords": [
		"hexadrop",
		"DDD",
		"Hexagonal",
		"Architecture"
	],
	"homepage": "https://github.com/hexatool/hexadrop",
	"bugs": {
		"url": "https://github.com/hexatool/hexadrop/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hexatool/hexadrop"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"type": "module",
	"sideEffects": false,
	"exports": {
		"./class": {
			"import": {
				"types": "./dist/class.d.ts",
				"default": "./dist/class.js"
			}
		},
		"./nullable": {
			"import": {
				"types": "./dist/nullable.d.ts",
				"default": "./dist/nullable.js"
			}
		},
		"./primitives": {
			"import": {
				"types": "./dist/primitives.d.ts",
				"default": "./dist/primitives.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"./class": [
				"./dist/class.d.ts"
			],
			"./nullable": [
				"./dist/nullable.d.ts"
			],
			"./primitives": [
				"./dist/primitives.d.ts"
			]
		}
	},
	"scripts": {
		"prepublishOnly": "tsup"
	}
}
