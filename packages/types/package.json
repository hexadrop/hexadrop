{
	"name": "@hexadrop/types",
	"version": "2.0.0-beta.2",
	"description": "Hexagonal architecture utils library",
	"keywords": [
		"hexadrop",
		"DDD",
		"Hexagonal",
		"Architecture"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/hexadrop/hexadrop"
	},
	"homepage": "https://github.com/hexadrop/hexadrop",
	"bugs": {
		"url": "https://github.com/hexadrop/hexadrop/issues"
	},
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist",
		"src"
	],
	"exports": {
		"./abstract": {
			"import": {
				"types": "./dist/abstract.d.ts",
				"default": "./dist/abstract.js"
			}
		},
		"./class": {
			"import": {
				"types": "./dist/class.d.ts",
				"default": "./dist/class.js"
			}
		},
		"./instance": {
			"import": {
				"types": "./dist/instance.d.ts",
				"default": "./dist/instance.js"
			}
		},
		"./newable": {
			"import": {
				"types": "./dist/newable.d.ts",
				"default": "./dist/newable.js"
			}
		},
		"./nullable": {
			"import": {
				"types": "./dist/nullable.d.ts",
				"default": "./dist/nullable.js"
			}
		},
		"./primitives": {
			"import": {
				"types": "./dist/primitives.d.ts",
				"default": "./dist/primitives.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"./abstract": [
				"./dist/abstract.d.ts"
			],
			"./class": [
				"./dist/class.d.ts"
			],
			"./instance": [
				"./dist/instance.d.ts"
			],
			"./newable": [
				"./dist/newable.d.ts"
			],
			"./nullable": [
				"./dist/nullable.d.ts"
			],
			"./primitives": [
				"./dist/primitives.d.ts"
			]
		}
	},
	"scripts": {
		"prepublishOnly": "tsup"
	},
	"devDependencies": {
		"tsup": "8.0.2"
	}
}
