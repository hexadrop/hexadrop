{
	"name": "@hexadrop/core",
	"version": "0.0.6",
	"description": "Hexagonal architecture utils library",
	"keywords": [
		"hexadrop",
		"DDD",
		"Hexagonal",
		"Architecture"
	],
	"homepage": "https://github.com/hexatool/hexadrop",
	"bugs": {
		"url": "https://github.com/hexatool/hexadrop/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hexatool/hexadrop"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"module": "./dist/hexadrop-core.mjs",
	"umd:main": "dist/hexadrop-core.umd.min.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/hexadrop-core.mjs"
		},
		"./test": {
			"types": "./dist/test.d.ts",
			"default": "./dist/hexadrop-core-test.mjs"
		}
	},
	"typesVersions": {
		"*": {
			"test": [
				"dist/test.d.ts"
			]
		}
	},
	"scripts": {
		"build": "vite build && vite build --config vite.config.umd.ts",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"sinon": "15.0.1",
		"uuid": "9.0.0"
	},
	"devDependencies": {
		"@hexadrop/mother": "0.0.2",
		"@types/sinon": "10.0.13",
		"@types/uuid": "9.0.1"
	}
}
