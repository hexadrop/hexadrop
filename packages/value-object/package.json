{
	"name": "@hexadrop/value-object",
	"version": "2.0.0-beta.6",
	"description": "Hexagonal architecture utils library",
	"keywords": [
		"hexadrop",
		"DDD",
		"Hexagonal",
		"Architecture"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hexadrop/hexadrop.git"
	},
	"homepage": "https://github.com/hexadrop/hexadrop",
	"bugs": {
		"url": "https://github.com/hexadrop/hexadrop/issues"
	},
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist",
		"src"
	],
	"exports": {
		"./boolean": {
			"import": {
				"types": "./dist/boolean.value-object.d.ts",
				"default": "./dist/boolean.value-object.js"
			}
		},
		"./date": {
			"import": {
				"types": "./dist/date.value-object.d.ts",
				"default": "./dist/date.value-object.js"
			}
		},
		"./number": {
			"import": {
				"types": "./dist/number.value-object.d.ts",
				"default": "./dist/number.value-object.js"
			}
		},
		"./string": {
			"import": {
				"types": "./dist/string.value-object.d.ts",
				"default": "./dist/string.value-object.js"
			}
		},
		"./uuii": {
			"import": {
				"types": "./dist/uuii.value-object.d.ts",
				"default": "./dist/uuii.value-object.js"
			}
		},
		"./ulid": {
			"import": {
				"types": "./dist/ulid.value-object.d.ts",
				"default": "./dist/ulid.value-object.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"./boolean": [
				"./dist/boolean.value-object.d.ts"
			],
			"./date": [
				"./dist/date.value-object.d.ts"
			],
			"./number": [
				"./dist/number.value-object.d.ts"
			],
			"./string": [
				"./dist/string.value-object.d.ts"
			],
			"./uuii": [
				"./dist/uuii.value-object.d.ts"
			],
			"./ulid": [
				"./dist/ulid.value-object.d.ts"
			]
		}
	},
	"scripts": {
		"prepublishOnly": "tsup"
	},
	"peerDependencies": {
		"ulid": "^2"
	},
	"peerDependenciesMeta": {
		"ulid": {
			"optional": true
		}
	},
	"dependencies": {
		"@hexadrop/error": "2.0.0-beta.2"
	},
	"devDependencies": {
		"@faker-js/faker": "8.4.1",
		"tsup": "8.0.2",
		"ulid": "2.3.0"
	}
}
